{
  "name": "tdd-auth",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "scripts": {
    "build": "npm run clean && tsc --build",
    "clean": "rimraf ./build",
    "build:swc": "npm run clean && swc ./src -s -d build",
    "start": "node build/index.js",
    "start:prod": "node --conditions=production build/index.js",
    "dev": "NODE_ENV=dev node --no-warnings=ExperimentalWarning --loader ts-node/esm src/index.ts",
    "dev:watch": "NODE_ENV=dev node --watch --no-warnings=ExperimentalWarning --loader ts-node/esm src/index.ts",
    "dev:swc:watch": "SWCRC=true NODE_ENV=dev node --watch --no-warnings=ExperimentalWarning --loader @swc-node/register/esm src/index.ts",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "test": "NODE_ENV=test node --test --no-warnings=ExperimentalWarning --loader ts-node/esm",
    "test:watch": "NODE_ENV=test npm run test -- --watch",
    "test:debug": "NODE_ENV=test TS_NODE_DEBUG=true npm run test",
    "typeorm": "typeorm-ts-node-esm",
    "migration:show:dev": "NODE_ENV=dev npm run typeorm migration:show -- -d src/database/data-source.ts",
    "migration:run:dev": "NODE_ENV=dev npm run typeorm migration:run -- -d src/database/data-source.ts",
    "migration:run:test": "NODE_ENV=test npm run typeorm migration:run -- -d src/database/data-source.ts",
    "migration:revert:dev": "NODE_ENV=dev npm run typeorm migration:revert -- -d src/database/data-source.ts",
    "migration:revert:test": "NODE_ENV=test npm run typeorm migration:revert -- -d src/database/data-source.ts",
    "migration:generate:dev": "NODE_ENV=dev npm run typeorm migration:generate -- -d src/database/data-source.ts",
    "migration:generate:test": "NODE_ENV=test npm run typeorm migration:generate -- -d src/database/data-source.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "imports": {
    "#*": {
      "production": "./build/*",
      "default": "./src/*"
    }
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.68",
    "@swc/helpers": "^0.5.1",
    "@swc/jest": "^0.2.27",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@tsconfig/node20": "^1.0.2",
    "@types/chai": "^4.3.5",
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.1",
    "@types/nodemailer": "^6.4.8",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "chai": "^4.3.7",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^36.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.6.1",
    "regenerator-runtime": "^0.13.11",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "dedent-js": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "inversify": "^6.0.1",
    "nodemailer": "^6.9.4",
    "pg": "^8.11.1",
    "purify-ts": "^2.0.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17",
    "zod": "^3.21.4"
  }
}
